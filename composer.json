{
    "name": "a2insights/octo-core",
    "description": "This is a part of octo system",
    "keywords": [
        "octo"
    ],
    "homepage": "https://github.com/a2ensite/octo-core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Atila Silva",
            "email": "atila.danvi@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "blade-ui-kit/blade-heroicons": "^0.3.1",
        "blade-ui-kit/blade-icons": "^0.5.1",
        "blade-ui-kit/blade-zondicons": "^0.2.0",
        "hieu-le/active": "^8.0.0",
        "illuminate/support": "^8.0",
        "yoeunes/notify": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Octo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octo\\OctoServiceProvider"
            ]
        }
    }
}
