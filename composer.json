{
    "name": "a2insights/octo-core",
    "description": "This is a part of octo system",
    "keywords": [
        "laravel",
        "php",
        "octo",
        "sass"
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/a2ensite/octo-core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Atila Silva",
            "email": "atila.danvi@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "bezhansalleh/filament-shield": "2.4",
        "filament/filament": "^2.17",
        "filament/spatie-laravel-settings-plugin": "^2.0",
        "illuminate/support": "^10.10",
        "livewire/livewire": "^2.12",
        "spatie/laravel-data": "^3.6",
        "spatie/laravel-enum": "^3.0",
        "spatie/laravel-stats": "^2.1",
        "symfony/intl": "^6.2"
    },
    "require-dev": {
        "laravel/pint": "dev-main",
        "orchestra/testbench-core": "^8.0",
        "phpunit/phpunit": "^9.3"
    },
    "suggest": {
        "doctrine/dbal": "Required by Cashier to modify tables in the database.",
        "laravel/cashier": "Required to use with the Stripe scaffolding."
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Octo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octo\\OctoServiceProvider"
            ]
        }
    }
}
