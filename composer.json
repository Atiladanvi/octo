{
    "name": "a2insights/octo-core",
    "description": "This is a part of octo system",
    "keywords": [
        "laravel",
        "php",
        "octo",
        "sass"
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/a2ensite/octo-core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Atila Silva",
            "email": "atila.danvi@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "blade-ui-kit/blade-heroicons": "^0.3",
        "blade-ui-kit/blade-icons": "^0.5",
        "blade-ui-kit/blade-zondicons": "^0.2",
        "hieu-le/active": "^8.0",
        "illuminate/support": "^8.0",
        "joelbutcher/socialstream": "^3.0",
        "laravel/jetstream": "^2.2",
        "livewire-ui/modal": "dev-main",
        "livewire/livewire": "^2.4",
        "spatie/laravel-newsletter": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Octo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octo\\OctoServiceProvider"
            ]
        }
    }
}
